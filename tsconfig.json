{
  "compilerOptions": {
    "outDir": "./build/",
    "target": "es5",
    "lib": ["DOM", "ES6"],
    "module": "commonjs",
    "allowJs": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmitOnError": true
  },
  "include": [
    "src"
  ],
  "compileOnSave": false
}
